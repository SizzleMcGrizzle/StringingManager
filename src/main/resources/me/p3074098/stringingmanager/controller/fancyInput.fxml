            <TextField fx:id="textField" prefHeight="30.0" prefWidth="268.0" promptText="Prompt">
               <HBox.margin>
                  <Insets bottom="6.0" left="10.0" right="10.0" top="6.0" />
               </HBox.margin>
               <styleClass>
                  <String fx:value="background-1" />
                  <String fx:value="text-fill-1" />
               </styleClass>
            </TextField>
         </children>
      </HBox>
      <Label fx:id="label" contentDisplay="CENTER" layoutX="19.0" layoutY="10.0" prefHeight="15.0" prefWidth="59.0" text="Label">
         <padding>
            <Insets left="5.0" right="5.0" />
         </padding>
         <font>
            <Font size="10.0" />
         </font>
         <styleClass>
            <String fx:value="background-1" />
            <String fx:value="text-fill-1" />
         </styleClass>
      </Label>
   </children>
   <cursor>
      <Cursor fx:constant="TEXT" />
   </cursor>
</Pane>
